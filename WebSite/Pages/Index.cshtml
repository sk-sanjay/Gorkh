        {
        @foreach (var item in Model.ModelBannerVms)
            {
        <div class="item" style="background-image:url('@($"{_config["BaseUrl"]}/img/banners/{item.BannerImage}")');" alt="@item.BannerImage" loading="lazy" asp-append-version="true"></div>


            }
        }

    </div>
</section>

<section class="search-sec">
    <div class="container">
        <vc:search-headers></vc:search-headers>
    </div>
</section>

<section class="service-sec">
    <div class="container">

        <div class="owl-carousel service-slide">
            @if (Model.modelcatimg != null)
            {
               @foreach (var item in Model.modelcatimg)
            {
            <div class="item">
                <div class="service-slide-single">
                    <div class="service-slide-left">
                        <img src="@($"{_config["SiteUrl"]}/img/products/{item.ProductImage}")" alt="@item.ProductImage" loading="lazy" asp-append-version="true">
                    </div>
                    <div class="service-slide-right">
                        <h2>@item.CategoryName</h2>
                    </div>
                    <a asp-page="ProductDetails" asp-route-id="@item.Id"><i class="icon-icon-15"></i></a>
                </div>
            </div>
            }
            }


        </div>
    </div>
</section>

<section class="sec-product">
    <div class="container">
        <div class="row">
            <div class="col-lg-3">
                <partial name="_MenuHomeLeftPartial" model="Model.ModelVms" />
                <a href="http://www.blspolymers.com/" target="_blank">
                    <div class="addSpace mt-2 mb-2"><img src="../img/Adds/Add_BLS.jpg" alt="BLS Polymers" asp-append-version="true"></div>
                </a>
                <div class="product-cat-link mt-2" style="background: #987e37;">
                    <a href="/buyerrequirement"><button type="button" id="btnSearch" class="btn btn-lg text-white" style="width:100%;"><p class="text-dark">Cannot find your requirement!<br /> Please upload your need here<br>We will locate the seller.</p></button></a>

                </div>
                <div class="card side-product">

                    <ul class="p-4" style="cursor:pointer";>
                        @if (Model.BuyerRequirements!=null)
                        {
                             @foreach (var item in Model.BuyerRequirements)
                        {
                        <li>
                            <a data-toggle="modal" data-target="#exampleModal12" class="BuyerReqdetails link-bdr" data-id="@item.Id"><p style="text-overflow: ellipsis; white-space: nowrap; overflow: hidden;">@item.ProductListingTitle</p></a>
                            <small class="dates"><i class="fa fa-calendar" aria-hidden="true"></i> @item.CreatedDate</small>
                        </li>
                        }
                        }

                    </ul>

                    <a class="descLink" href="/buyerrequirement-details"><span>View All</span></a>

                </div>


            </div>

            <div class="col-lg-9">
                <div class="sec-product-right">
                    <h2 class="alt">Featured Machines</h2>
                    <div class="owl-carousel pro-slide">
