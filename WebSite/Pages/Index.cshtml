                        <div class="item">
                            <div class="service-slide-single">
                                <div class="service-slide-left">
                                    <img src="@($"{_config["BaseUrl"]}/img/OurCustomersLogo/{item.Logo}")" alt="@item.CompanyName" loading="lazy" asp-append-version="true">
                                </div>
                            </div>
                        </div>
                        }
                        }


                    </div>
                </div>
            </section>

        </div>

    </div>

</section>
