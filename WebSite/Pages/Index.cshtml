                        {       @foreach (var item in Model.modellatestproduct)
                        {
                        <div class="item">
                            <div class="pro-slide-single">
                                <a data-id="@item.Id" class="details">
                                    <div class="pro-slide-img"><img src="@($"{_config["SiteUrl"]}/img/products/{item.ProductImage}")" alt="@item.ProductImage" loading="lazy" asp-append-version="true"></div>
                                </a>
                                <div class="pro-slide-txt">
                                    <div class="prolike">
                                        <a data-id="@item.Id" class="favorite"><i class="fa fa-heart" aria-hidden="true"></i><i class="fa fa-heart-o" aria-hidden="true"></i></a>
                                    </div>
                                    <h3><a asp-page="ProductDetails" asp-route-id="@item.Id">@item.SubSubCategoriesName</a></h3>
                                    <h4>@item.StateName</h4>
                                    <h5>
                                        @{
                                                decimal price = item.EstimatePrice;
                                                System.Globalization.CultureInfo Indian = new System.Globalization.CultureInfo("hi-IN");
                                        }
                                        @(String.Format(Indian, "{0:C0}", price))
                                    </h5>
                                    <p>Code: @item.ProductNumber</p>
                                    <p>Quantity: @item.QuantityAvl</p>
                                </div>
                            </div>
                            <a class="descLink" asp-page="ProductDetails" asp-route-id="@item.Id"><span>Description</span> <i class="fa fa-angle-down" aria-hidden="true"></i></a>

                        </div>
                        }

                        }




                    </div>

                </div>

                <a href="https://bioresurge.in/" target="_blank">
                    <div class="addSpace mt-2 mb-2"><img src="../img/Adds/Add_BioReSurge.jpg" alt="Bio ReSurge" asp-append-version="true"></div>
                </a>

            </div>
            <section class="testimonial text-center" style="display:none">
                <div class="container">

                    <div class="headingtestimonial   white-heading">
                        Testimonial
                    </div>
                    <div id="testimonial4" class="carousel slide testimonial4_indicators testimonial4_control_button thumb_scroll_x swipe_x" data-ride="carousel" data-pause="hover" data-interval="5000" data-duration="2000">

                        <div class="carousel-inner" role="listbox">
                            @if (Model.Testimonials!=null)
                            {
                                @foreach (var item in Model.Testimonials)
                            {
                            <div class="carousel-item">
                                <div class="testimonial4_slide">
                                    <img class="img-circle img-responsive" src="@($"{_config["BaseUrl"]}/img/users/{item.Image}")" alt="@item.Image" loading="lazy" asp-append-version="true">
                                    <p class="text-dark">@item.Description<br /><b>@item.CompanyName/@item.Designtion/@item.Name/@item.Location</b></p>

                                </div>
                            </div>
                            }
                            }


                        </div>
                        <a class="carousel-control-prev" href="#testimonial4" data-slide="prev">
                            <span class="carousel-control-prev-icon"></span>
                        </a>
                        <a class="carousel-control-next" href="#testimonial4" data-slide="next">
                            <span class="carousel-control-next-icon"></span>
                        </a>
                    </div>
                </div>
            </section>
