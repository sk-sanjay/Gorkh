@*@model ProductsBySellerVM*@
<div class="modal fade" id="ManageModal" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="ManageModalTitle" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <form>
            @*<input type="hidden" asp-for="Id" />*@
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="ManageModalTitle">Total Views</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <label class="col-form-label">Total Views</label>
                    </div>
                    <div class="form-group">
                        <label class="col-form-label">Unique Views</label>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>