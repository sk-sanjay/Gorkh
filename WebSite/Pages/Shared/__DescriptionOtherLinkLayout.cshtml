@model Application.ViewModels.CommonVM

@if (!string.IsNullOrEmpty(Model.Content))
{
    @Html.Raw(Model.Content)
}
@*else
{
    if (System.IO.Path.GetExtension(Model.Link).ToLower() != ".pdf")
    {
        <img src="https://admin.hpcl.businesstowork.com/img/UploadedFiles/MenuHeading/Files/English/@Model.Link" width="100%" alt="@Model.Link" />
    }
    else
    {
        <div class="col-sm-12">
            <iframe src="https://admin.hpcl.businesstowork.com/img/UploadedFiles/MenuHeading/Files/English/@Model.Link" style="width:100%;height:100vw"></iframe>
        </div>
    }


}*@
